    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "harbour-dbg",
            "request": "launch",
            "name": "Launch Harbour Program",
            "windows":{
                "program": "${workspaceFolder}\\msvc64\\debug\\${workspaceFolderBasename}.exe",
                "sourcePaths": [
                    "${workspaceFolder}"
                ]
            },
            "linux":{
//_M_ Fix path
                "program": "/etc/Harbour_WebView/HelloWorld.exe",
                "sourcePaths": [
                    "${workspaceFolder}"
                ]
            }
        }
    ]
}
